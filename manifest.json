{
  "name": "TierHacker",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["*://*.tiermaker.com/*"],
      "js": ["content.js"],
      "css": ["tierhacker.css"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "Tierhacker_16.png",
    "48": "Tierhacker_48.png",
    "128": "Tierhacker_128.png"
  }
}
